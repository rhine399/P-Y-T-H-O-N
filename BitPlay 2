def TwoOdd(arr, size):
    xorof2 = arr[0]
    x = 0
    y = 0
    SetBit = 0
    for i in range(1,size):

      xorof2 = xorof2 ^ arr[i]
      SetBit = xorof2 & ~(xorof2-1)
      for i in range(size):
         if(arr[i]& SetBit):
            x = x ^ arr[i]
         x = x ^ arr[i]

    else:
         y = y ^ arr[i]

    print("TwoOdd elements are",x, "&",y)

arr = []
arr_size = int(input("Enters the size of the array:"))   
for i in range(0,arr_size):
   z = int(input("Enter element:"))
   arr.append(z)

print("TwoOdd")   
